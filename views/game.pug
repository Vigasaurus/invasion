extends layout-no-menu

block content
	if username
		section#game-container(class=`username-${username}`)
		script.
			var settings = {
				timestampsEnabled: #{timestampsEnabled},
				helpDisabled: #{helpDisabled}
			};
	else
		section#game-container
	script.
		var gameList = {}
		gameList.list = !{JSON.stringify(gameList).replace(/<\//g, '<\\/')}